pipeline {
     agent any
   stages {
       stage ('compile') {
	    steps {
		   sh '/usr/local/maven/bin/mvn compile'
		}
	}
	  stage ('test') {
	   steps {
		       sh '/usr/local/maven/bin/mvn test'
		   }
	   }
	  stage ('package') {
	   steps {
	           sh '/usr/local/maven/bin/mvn package'
	   }
	   }
	   stage ('install') {
	   steps {
	           sh '/usr/local/maven/bin/mvn install'
	  }
	  }
	  stage ('deploy') {
	   steps {
	           sh '/usr/local/maven/bin/mvn deploy'
	  }
}
}
}
