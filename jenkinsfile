pipeline {
     agent any
   stages {
       stage ('compile stage') {
	    steps {
		   sh '/usr/local/maven/bin/mvn clean compile'
		}
	}
	  stage ('testing stage') {
	   steps {
		       sh '/usr/local/maven/bin/mvn test'
		   }
	   }
	  stage ('deployment') {
	   steps {
	           sh '/usr/local/maven/bin/mvn deploy'
	   }
	   }
	  }
	}
